algoritmo "semnome"
// Leia 2 numeros e apresente a décima parte da
// subtraçao entre eles, usando uma terceira variavel.
var
n1, n2, subtracao: real

inicio
escreval("Escreva um numero: ")
leia (n1)
escreval("Escreva outro numero: ")
leia (n2)

subtracao <- (n1-n2)/10
escreva ("O valor da decima parte é: ", subtracao)

fimalgoritmo